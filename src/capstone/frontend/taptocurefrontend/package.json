  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "redux-mock-store": "^1.5.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/Subcomponents/About.jsx",
      "src/components/Subcomponents/FeatureBox.jsx",
      "src/components/Subcomponents/Feauture.jsx",
      "src/components/Subcomponents/Footer.jsx",
      "src/components/Subcomponents/Hero.jsx",
      "src/components/Subcomponents/WhyChooseUs.jsx",
      "src/components/Landing.jsx",
      "src/components/Doctor/DoctorAppointmentHistory.jsx",
      "src/components/Patient/AppointmentHistory.jsx",
      "src/components/Auth/ForgotPasswordModal.jsx",
      "src/components/Auth/ProtectedRoute.jsx",
      "src/components/Admin/AdminDashboard.jsx",
      "src/components/Admin/Doctors.jsx",
      "src/components/Admin/Patients.jsx"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "src/components/Admin/(?!AdminDashboard.jsx|Doctors.jsx|Patients.jsx).*"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
